{
  "name": "radix-select",
  "version": "0.1.5",
  "description": "Selectively installs Radix Primitive components in bulk in one go  ",
  "main": "./dist/index.js",
  "type": "module",
  "bin": {
    "radix-select": "./dist/index.js"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc && node ./dist/index.js",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpererdogan8/radix-ui-cli.git"
  },
  "keywords": [
    "radix",
    "radix-ui",
    "radix-select",
    "ui",
    "components",
    "select",
    "selectable",
    "list",
    "installer",
    "remover"
  ],
  "author": "Alper Erdogan <alpererdogan8@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alpererdogan8/radix-ui-cli/issues"
  },
  "homepage": "https://github.com/alpererdogan8/radix-ui-cli#readme",
  "dependencies": {
    "chalk": "^5.2.0",
    "child_process": "^1.0.2",
    "inquirer": "^9.2.7",
    "nanospinner": "^1.1.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  }
}
